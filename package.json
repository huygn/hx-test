    "create-react-class": "^15.6.3",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-is": "^16.6.3"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.7.6"
  }
}
